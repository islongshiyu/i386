<p><% if (config.author){ %>
  &copy; <%= new Date().getFullYear() %> <%= config.author %>
  <% } else { %>
  &copy; <%= new Date().getFullYear() %> <%= config.title %>
  <% } %>    
</p>
<script src="<%- config.root %>js/mermaid.min.js"></script>
 <script>
  if (window.mermaid) {
    mermaid.initialize({theme: 'neutral', useMaxWidth: true});
  }
</script>
